
static()
  position: static

relative()
  position: relative

absolute()
  position: absolute

fixed()
  position: fixed

fixed-spread()
  position: fixed
  top: 0
  right: 0
  bottom: 0
  left: 0
  width: 100%
  height: 100%
  
reset-list-styles()
  list-style: none
  margin: 0
  padding: 0
  
center-center()
  float: none //
  top: 50%
  left: 50%
  transform: translate(-50%, -50%)

absolute-transform-center()
  center-center()
  position: absolute

fixed-transform-center()
  center-center()
  position: fixed
